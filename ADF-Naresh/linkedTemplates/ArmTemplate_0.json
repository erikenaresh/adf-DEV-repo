				"type": "CosmosDb",
				"typeProperties": {
					"connectionString": "[parameters('LS_CosmosDbNoSl_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/LS_DFtext')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
